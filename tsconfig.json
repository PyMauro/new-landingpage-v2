{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "types": ["@sanity/astro/module"],
     "paths": {
    "@components/*": ["src/components/*"],
    "@assets/*": ["src/assets/*"]
  }
  }
}